{
  "name": "marks3",
  "version": "0.1.0",
  "description": "A serverless markdown wiki system hosted on AWS S3 with Cognito authentication",
  "type": "module",
  "packageManager": "pnpm@8.0.0",
  "scripts": {
    "setup:github": "node scripts/setup-github.js",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "install:deps": "pnpm install",
    "deploy": "node scripts/deploy.js deploy",
    "deploy:destroy": "node scripts/deploy.js destroy",
    "terraform:validate": "node scripts/validate-terraform.js",
    "terraform:init": "cd terraform && terraform init",
    "terraform:plan": "cd terraform && terraform plan",
    "terraform:apply": "cd terraform && terraform apply",
    "terraform:destroy": "cd terraform && terraform destroy"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "assert": "^2.1.0",
    "buffer": "^6.0.3",
    "chalk": "^5.3.0",
    "crypto-browserify": "^3.12.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "events": "^3.3.0",
    "inquirer": "^9.2.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "readable-stream": "^4.7.0",
    "simple-git": "^3.20.0",
    "stream-browserify": "^3.0.0",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "vite": "^5.0.3",
    "vitest": "^1.2.0",
    "vm-browserify": "^1.1.2"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^3.910.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.910.0",
    "@aws-sdk/client-s3": "^3.910.0",
    "@aws-sdk/credential-providers": "^3.910.0",
    "@aws-sdk/types": "^3.910.0",
    "@milkdown/core": "^7.0.0",
    "@milkdown/plugin-listener": "^7.0.0",
    "@milkdown/preset-commonmark": "^7.0.0",
    "@milkdown/theme-nord": "^7.0.0",
    "dompurify": "^3.3.0"
  },
  "keywords": [
    "wiki",
    "markdown",
    "serverless",
    "aws",
    "s3",
    "cognito",
    "svelte",
    "typescript"
  ],
  "author": "MarkS3 Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YOUR_USERNAME/marks3.git"
  },
  "bugs": {
    "url": "https://github.com/YOUR_USERNAME/marks3/issues"
  },
  "homepage": "https://github.com/YOUR_USERNAME/marks3#readme",
  "engines": {
    "node": ">=18.0.0"
  }
}